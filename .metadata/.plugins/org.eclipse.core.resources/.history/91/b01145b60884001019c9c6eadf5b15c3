package baekjoon_250826;

import java.util.Scanner;
import java.util.ArrayList;

public class No10811 {

	public static void main(String[] args) {
		Scanner sc = new Scanner(System.in);
		
		int n = sc.nextInt();
		int m = sc.nextInt();
		ArrayList<Integer> basket = new ArrayList<>();
		
		for (int i=1; i <= n; i++) {
			basket.add(i);
		}
		
		for (int j=1; j <= m; j++) {
			int start = sc.nextInt();
			int end = sc.nextInt();
			int idxstart = start-1;
			int idxend = end-1;
			int loopnum = (end-start+1) / 2;
			for (int k = 0; k < loopnum; k++) {
				int temp = basket.get(idxstart);
				basket.set(idxstart, basket.get(idxend));
				basket.set(idxend, temp);
				idxstart += 1 ;
				idxend -= 1;
			}
		}
	
		System.out.println(basket);
		
		sc.close();

	}

}
