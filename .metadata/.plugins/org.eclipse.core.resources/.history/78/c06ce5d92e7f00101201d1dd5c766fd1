package baekjoon_250822;

import java.util.Scanner;

public class No10952 {

	public static void main(String[] args) {
		Scanner sc = new Scanner(System.in);
		int t = sc.nextInt();
		StringBuilder sb = new StringBuilder(2*t);
		

		for(int i = 1; i<=t; i++) {
			int num1 = sc.nextInt();
			int num2 = sc.nextInt();
			sb.append(num1+num2);
			sb.append("\n");
		}
		
		System.out.println(sb);
		
		sc.close();

	}

}
